{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Python Debugger-main script",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      //"justMyCode": false
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Node Debugger-mongo upload",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/uploadSensorData/uploadMongo.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Node Debugger-firebase upload",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/uploadSensorData/uploadFirebase.js"
    }
  ]
}